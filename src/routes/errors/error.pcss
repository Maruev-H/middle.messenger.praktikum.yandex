.error {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;

  h1 {
    font-size: 40px;
    font-weight: 500;
    line-height:  40px;
    margin-bottom: 20px;
  }

  h2 {
    font-size: 20px;
    font-weight: 500;
    line-height: 20px;
    margin-bottom: 69px;
  }

  h3 {
    font-size: 11px;
    font-weight: 500;
    line-height: 11px;
    
    a {
        color: rgba(51, 105, 243, 1);
    }
  }
}
